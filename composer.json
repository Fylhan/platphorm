{
    "name": "fylhan/platphorm",
    "description": "Platphorm is a proof of concept of a PHP platform able to install gracefully PHP apps (like SHaarli, Blogotext, Leed and KrissFeed) using Composer.",
    "authors": [
        {
            "name": "Fylhan",
            "email": "bnmaster@la-bnbox.fr"
        }
    ],
    "repositories": [
	    {
	        "type":"vcs",
	        "url":"https://github.com/Fylhan/multiple-regex-tool.git"
	    }
	],
    "require": {
		"fylhan/multiple-regex-tool":"*"
    },
    "minimum-stability":"dev",
    "scripts": {
		"post-install-cmd": "MyVendor\\MyClass::postUpdate",
		"post-update-cmd": "MyVendor\\MyClass::postInstall"
	}
}
